
@{
    ViewBag.Title = "Creación masiva";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (@Html.BeginForm("BulkCreate", "Usuarios", FormMethod.Post,
 new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
            <div class="form-group">
                <a href="@Url.Content("~/Content/Plantilla.txt")" download>Descargar plantilla de ejemplo</a>
            </div>
        <div class="form-group">
            @Html.Label("Seleccione un archivo")
        <input type="file" name="file" id="file" />
    </div>

    <div class="form-group">
        <input type="submit" class="btn btn-primary" value="Leer archivo" name="submit" />
    </div>
</div>
}
