@model Asistencia.Models.Evento

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div class="container">
        <h3>Esta seguro de eliminar este evento?</h3>

        <h4>@Html.DisplayFor(model => model.strTema)</h4>
        <hr />

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-primary" />
                <input type="button" value="Volver" onclick="location.href='@Url.Action("Index","Eventos")'" class="btn btn-default" />

            </div>
        }
    </div>
